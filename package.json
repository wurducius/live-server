{
  "name": "eofol-live-server",
  "version": "1.2.4",
  "description": "simple development http server with live reload capability",
  "keywords": [
    "front-end",
    "development",
    "tool",
    "server",
    "http",
    "cli"
  ],
  "author": "Jakub Eliáš",
  "dependencies": {
    "chokidar": "3.6.0",
    "colors": "1.4.0",
    "connect": "^3.7.0",
    "cors": "latest",
    "event-stream": "^4.0.1",
    "faye-websocket": "^0.11.4",
    "http-auth": "^4.2.0",
    "morgan": "^1.10.0",
    "object-assign": "latest",
    "open": "latest",
    "proxy-middleware": "latest",
    "send": "latest",
    "serve-index": "^1.9.1"
  },
  "devDependencies": {
    "eslint": "^9.7.0",
    "supertest": "^7.0.0"
  },
  "scripts": {
    "lint": "eslint live-server.js index.js"
  },
  "main": "index.js",
  "bin": {
    "live-server": "./live-server.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/wurducius/eofol-live-server.git"
  },
  "node": ">=18",
  "preferGlobal": true,
  "license": "MIT",
  "eslintConfig": {
    "env": {
      "node": true
    },
    "rules": {
      "quotes": 0,
      "curly": 0,
      "strict": 0,
      "no-process-exit": 0,
      "eqeqeq": 1,
      "no-unused-vars": 1,
      "no-shadow": 1
    }
  }
}
